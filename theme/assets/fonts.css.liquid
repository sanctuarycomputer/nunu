@font-face {
  font-family: "ITCGaramondLight";
  font-style: normal;
  font-weight: 300;
  src: url('{{ "itc-garamond-light.eot" | asset_url }}');
  src: url('{{ "itc-garamond-light.woff2" | asset_url }}') format("woff2"),
    url('{{ "itc-garamond-light.woff" | asset_url }}') format("woff"),
    url('{{ "itc-garamond-light.svg#svgFontName" | asset_url }}') format("svg");
}

@font-face {
  font-family: "ABCDiatypeLight";
  font-style: normal;
  font-weight: 300;
  src: url('{{ "ABCDiatype-Light.woff2" | asset_url }}') format("woff2"),
    url('{{ "ABCDiatype-Light.woff" | asset_url }}') format("woff"),
    url('{{ "ABCDiatype-Light.svg#svgFontName" | asset_url }}') format("otf");
}
