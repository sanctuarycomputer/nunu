@font-face {
  font-family: "ITCGaramondStdLtCond";
  font-style: normal;
  font-weight: 300;
  font-display: swap;
  src: url('{{ "ITCGaramondStdLtCond.eot" | asset_url }}');
  src: url('{{ "ITCGaramondStdLtCond.woff2" | asset_url }}') format("woff2"),
    url('{{ "ITCGaramondStdLtCond.woff" | asset_url }}') format("woff"),
    url('{{ "ITCGaramondStdLtCond.woff.svg#svgFontName" | asset_url }}') format("otf");
}

